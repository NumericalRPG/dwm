{
   "background": {
      "scripts": [ "scripts/background.js", "scripts/functions.js" ]
   },
   "browser_action": {
      "default_icon": "icons/off.png",
      "default_popup": "popup.html",
      "default_title": "Настройка прокси"
   },
   "commands": {
      "offProxy": {
         "description": "Disable proxy",
         "global": true,
         "suggested_key": {
            "default": "Ctrl+Shift+0"
         }
      },
      "onProxy": {
         "description": "Enable proxy",
         "global": true,
         "suggested_key": {
            "default": "Ctrl+Shift+1"
         }
      },
      "options": {
         "description": "Open option page",
         "global": true,
         "suggested_key": {
            "default": "Ctrl+Shift+5"
         }
      }
   },
   "default_locale": "en",
   "description": "Manage setting of connect of proxy in your browser so quickly and so easily as you want.",
   "icons": {
      "128": "icons/128x128.png",
      "16": "icons/16x16.png",
      "48": "icons/48x48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "ProxyControl",
   "options_page": "options.html",
   "permissions": [ "proxy", "storage", "unlimitedStorage", "\u003Call_urls>", "webRequest", "webRequestBlocking" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.4.7"
}
